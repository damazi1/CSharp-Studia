<Window x:Class="CSLab08.WpfApp.EditStudentGrade"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CSLab08.WpfApp"
        mc:Ignorable="d"
        Title="EditStudentGrade" Height="200" Width="200">
    <Grid>
        <Button Name="_AddGrade" Content="Add Grade" HorizontalAlignment="Left" Height="20" Margin="80,113,0,0" VerticalAlignment="Top" Width="95" Click="_AddGrade_Click"/>
        <TextBox Name="TextBoxSubject" HorizontalAlignment="Left" Height="19" Margin="80,37,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="95"/>
        <TextBox Name="TextBoxGrade" HorizontalAlignment="Left" Height="19" Margin="80,75,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="95"/>
        <Label Content="Subject: " HorizontalAlignment="Left" Margin="5,30,0,0" VerticalAlignment="Top"/>
        <Label Content="Grade: " HorizontalAlignment="Left" Margin="5,70,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
