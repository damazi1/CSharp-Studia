<Window x:Class="CSLab09.WpfApp.AddEditStudentWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CSLab09.WpfApp"
        mc:Ignorable="d"
        Title="AddEditStudentWindow" Height="300" Width="400">
    <Grid>
        <TextBox Name="TextBoxFirstName" HorizontalAlignment="Left" Margin="230,48,0,0" TextWrapping="Wrap" Text="(Imie)" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="TextBoxSurName" HorizontalAlignment="Left" Margin="230,102,0,0" TextWrapping="Wrap" Text="(Nazwisko)" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="TextBoxFaculty" HorizontalAlignment="Left" Margin="230,159,0,0" TextWrapping="Wrap" Text="(Wydzial)" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="TextBoxStudentNo" HorizontalAlignment="Left" Margin="230,217,0,0" TextWrapping="Wrap" Text="(Nr_Indeksu)" VerticalAlignment="Top" Width="120"/>
        <DatePicker Name="DatePickerDateOfBirth" HorizontalAlignment="Left" Margin="33,48,0,0" VerticalAlignment="Top" Width="167"/>
        <Button Name="ButtonSaveStudent" Content="Dodaj / Edytuj Studenta" HorizontalAlignment="Left" Margin="33,215,0,0" VerticalAlignment="Top" Click="ButtonSaveStudent_Click" Width="167"/>

    </Grid>
</Window>
